{
  "manifest_version": 3,
  "name": "Emails Extractor",
  "version": "1.0",
  "description": "Extract emails from a webpage",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  }
}